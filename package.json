{
  "private": true,
  "version": "4.1.0",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public vendor/getkirby/cms/router.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build && node scripts/buildServiceWorker.mjs",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "ohmyfetch": "^0.4.18",
    "scule": "^0.2.1",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.2.1",
    "consola": "^2.15.3",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.0.1",
    "nanoid": "^3.3.4",
    "prettier": "2.6.2",
    "shx": "^0.3.4",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9"
  }
}
