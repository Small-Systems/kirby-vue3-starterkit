{
  "name": "kirby-vue3-starterkit",
  "version": "1.0.0",
  "scripts": {
    "kirby:serve": "node serve-kirby.js",
    "dev": "vite",
    "build": "vite build && rm -rf public/assets && mv frontend/dist/assets public/assets",
    "start": "concurrently \"npm run kirby:serve\" \"npm run dev\""
  },
  "dependencies": {
    "idb-keyval": "^3.2.0",
    "vue": "^3.0.0-beta.18",
    "vue-router": "^4.0.0-alpha.14"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-beta.18",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "php-server": "^0.2.1",
    "vite": "^1.0.0-beta.8"
  },
  "browserslist": [
    "last 2 Android versions",
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 iOS versions",
    "last 2 Safari versions",
    "last 2 Samsung versions"
  ]
}
